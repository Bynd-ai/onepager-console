# Example environment variables for the One-Pager Admin Console
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required for database connection)
SUPABASE_URL="your-supabase-url-here"
SUPABASE_ANON_KEY="your-supabase-anon-key-here"

# Optional: Other API keys (if needed for additional features)
OPENAI_API_KEY="your-openai-api-key-here"
AZURE_STORAGE_CONNECTION_STRING="your-azure-storage-connection-string-here"
AZURE_STORAGE_CONTAINER="your-container-name-here"

# Database Configuration (if using local database)
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=report_building_app

# Backend Configuration
BACKEND_URL=http://localhost:5005
PORT=5005
